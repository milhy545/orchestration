# MCP Orchestration System Environment Variables
# Copy this file to .env and fill in your actual values

# Database Configuration
POSTGRES_DB=mcp_unified
POSTGRES_USER=mcp_admin
POSTGRES_PASSWORD=change_me_in_production
MCP_DATABASE_URL=postgresql://mcp_admin:change_me_in_production@postgresql:5432/mcp_unified
DEFAULT_POSTGRES_URL=postgresql://mcp_admin:change_me_in_production@postgresql:5432/mcp_unified

# Redis Configuration
REDIS_URL=redis://redis:6379

# Shared JWT secret for internal auth-enabled services (marketplace + security-mcp)
JWT_SECRET=change_me_market_jwt

# API Keys (get your keys from respective providers)
GEMINI_API_KEY=your_gemini_api_key_here
PERPLEXITY_API_KEY=your_perplexity_api_key_here

# MCP Server Configuration
MCP_SERVER_PORT=8000
QDRANT_URL=http://qdrant-vector:6333

# Host path configuration (portable docker-compose mounts)
# PROJECT_ROOT: absolute path to this repository on the Docker host
# DATA_ROOT: directory for persistent data mounts (postgres, redis, qdrant, etc.)
# MONITORING_ROOT: base path containing ./monitoring (useful when Docker daemon
#                  cannot access your workspace path directly, e.g. Coder remote daemon)
# HOST_HOME_PATH: host /home bind mount for filesystem-mcp read-only access

# Gmail Configuration
GMAIL_ADDRESS=your_email@gmail.com
GMAIL_APP_PASSWORD=your_gmail_app_password

PROJECT_ROOT=.
DATA_ROOT=./data
MONITORING_ROOT=.
HOST_HOME_PATH=/home
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
XAI_API_KEY=
OPENROUTER_API_KEY=
# ZEN MCP Configuration
GOOGLE_API_KEY=your_google_api_key_here
DIAL_API_KEY=your_dial_api_key_here
DIAL_API_HOST=https://dial-api-host.com
DIAL_API_VERSION=2023-12-01
CUSTOM_API_URL=http://localhost:1234/v1
CUSTOM_API_KEY=your_custom_api_key
CUSTOM_MODEL_NAME=local-model-v1
DISABLED_TOOLS=
MAX_MCP_OUTPUT_TOKENS=4096

# Marketplace MCP (Hybrid skills market + MCP subregistry)
MARKET_BASE_URL=http://localhost:7034
# Optional: pre-minted JWT token for orchestrator/wrapper outbound calls
MARKETPLACE_JWT_TOKEN=

# FORAI Configuration
FORAI_WORKSPACE_PATH=/workspace

# Vault Variant B (optional overlay)
VAULT_PORT=7070
VAULT_WEBUI_PORT=10000
VAULT_DEV_ROOT_TOKEN=dev-root-token

# Code Graph Configuration
NEO4J_URL=bolt://neo4j:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=neo4j_password
